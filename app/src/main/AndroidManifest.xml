<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="live.andiirham.kuylearn">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.KuyLearn">
        <activity android:name=".andi.ExploreActivity"></activity>
        <activity android:name=".hakkan.DashboardActivity" />
        <activity android:name=".hakkan.LoginActivity" />
        <activity android:name=".hakkan.RegisterActivity" />
        <activity
            android:name=".hakkan.WelcomeActivity"
            android:label="welcome_activity" />
        <activity
            android:name=".hakkan.SplashScreenActivity"
            android:theme="@style/SplashScreenStyle">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".hakkan.MainActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>